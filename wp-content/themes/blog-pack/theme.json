{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"customTemplates": [
		{
			"name": "blog",
			"postTypes": [
				"page"
			],
			"title": "Blog"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": true,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"text": true,
			"palette": [
				{
					"color": "#000000",
					"name": "Bold",
					"slug": "bold"
				},
				{
					"color": "#242925",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#959E95",
					"name": "Light",
					"slug": "light"
				},
				{
					"color": "#F5F5F5",
					"name": "Extra Light",
					"slug": "extra-light"
				},
				{
					"color": "#F0F2E9",
					"name": "Vibrant",
					"slug": "vibrant"
				},
				{
					"color": "#d2ddd2",
					"name": "Secondary Vibrant",
					"slug": "secondary-vibrant"
				}
			]
		},
		"layout": {
			"contentSize": "650px",
			"wideSize": "1200px"
		},
		"shadow": {
			"defaultPresets": true,
			"presets": []
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"increment": 1.5,
				"mediumStep": 1.5,
				"operator": "*",
				"steps": 0,
				"unit": "rem"
			},
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Hedvig Letters Serif",
					"name": "Hedvig Letters Serif",
					"slug": "hedvig-letters-serif-regular",
					"fontFace": [
						{
							"fontFamily": "Hedvig Letters Serif",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Hedvig-Letters-Serif.ttf"
							]
						}
					]
				},
				{
					"name": "Manrope",
					"slug": "manrope",
					"fontFamily": "Manrope",
					"fontFace": [
						{
							"fontFamily": "Manrope",
							"fontStyle": "normal",
							"fontWeight": "200 800",
							"src": "file:./assets/fonts/Manrope.ttf"
						}
					]
				},
				{
					"name": "NewsCycle-Bold",
					"slug": "news-cycle-bold",
					"fontFamily": "NewsCycle-Bold",
					"fontFace": [
						{
							"fontFamily": "NewsCycle-Bold",
							"fontStyle": "normal",
							"fontWeight": "200 800",
							"src": "file:./assets/fonts/NewsCycle-Bold.ttf"
						}
					]
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/search": {
				"elements": {
					"button": {
						"spacing": {
							"padding": {
								"bottom": "10px",
								"left": "20px",
								"right": "20px",
								"top": "10px"
							}
						}
					}
				}
			},
			"core/button": {
				"border": {
					"bottom": {
						"style": "none",
						"width": "0px"
					},
					"left": {
						"style": "none",
						"width": "0px"
					},
					"radius": "7px",
					"right": {
						"style": "none",
						"width": "0px"
					},
					"top": {
						"style": "none",
						"width": "0px"
					}
				},
				"spacing": {
					"padding": {
						"bottom": "10px",
						"left": "20px",
						"right": "20px",
						"top": "10px"
					}
				}
			},
			"core/comment-author-name": {
				"elements": {
					"link": {
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-date": {
				"elements": {
					"link": {
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "1rem"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "1rem"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "1rem"
				}
			},
			"core/comments-pagination": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"top": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)"
					}
				}
			},
			"core/comments-title": {
				"spacing": {
					"margin": {
						"bottom": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)"
					}
				},
				"typography": {
					"fontSize": "1.75rem"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "1rem"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "1rem"
				}
			},
			"core/post-date": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "1rem",
					"fontWeight": "400"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "1.125rem"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "1rem"
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "1.25rem",
						"top": "1.25rem"
					}
				},
				"typography": {
					"fontWeight": "400"
				}
			},
			"core/pullquote": {
				"border": {
					"style": "solid",
					"width": "1px 0"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "1rem",
							"fontStyle": "normal",
							"textTransform": "none"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem) !important",
						"top": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem) !important"
					}
				},
				"typography": {
					"lineHeight": "1.3"
				}
			},
			"core/query": {
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "2.25rem"
						}
					}
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "1rem",
					"fontWeight": "400"
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"color": "inherit",
						"style": "solid",
						"width": "1px"
					}
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "1rem",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "clamp(1.5rem, 5vw, 2rem)",
						"right": "clamp(1.5rem, 5vw, 2rem)"
					}
				}
			},
			"core/separator": {
				"css": " &:not(.is-style-wide):not(.is-style-dots):not(.alignwide):not(.alignfull){width: 100px}"
			},
			"core/site-title": {
				"elements": {
					"link": {
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "1.125rem",
					"fontWeight": "normal",
					"lineHeight": "1.4"
				}
			}
		},
		"color": {
			"background": "#ffffff",
			"text": "#000000"
		},
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "#000000",
						"text": "#ffffff"
					}
				},
				":focus": {
					"color": {
						"background": "#000000",
						"text": "#ffffff"
					}
				},
				":hover": {
					"color": {
						"background": "#000000",
						"text": "#ffffff"
					}
				},
				":visited": {
					"color": {
						"text": "#000000"
					}
				},
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "#000000",
					"text": "#ffffff"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "3.625rem",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(2.625rem, calc(2.625rem + ((1vw - 0.48rem) * 8.4135)), 3.25rem)",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "2.25rem"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.75rem"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1.125rem",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1.125rem",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "1.4",
					"fontFamily": "var(--wp--preset--font-family--hedvig-letters-serif-regular)"
				}
			},
			"link": {
				":active": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "#000000"
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": {
				"bottom": "0",
				"left": "clamp(1.5rem, 5vw, 2rem)",
				"right": "clamp(1.5rem, 5vw, 2rem)",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--manrope)",
			"fontSize": "1.125rem",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		},
		{
			"area": "uncategorized",
			"name": "post-meta",
			"title": "Post Meta"
		}
	]
}